# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=release
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release)
  MYLIB_config = release
endif

PROJECTS := MYLIB

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

MYLIB:
ifneq (,$(MYLIB_config))
	@echo "==== Building MYLIB ($(MYLIB_config)) ===="
	@${MAKE} --no-print-directory -C . -f MYLIB.make config=$(MYLIB_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f MYLIB.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   MYLIB"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"