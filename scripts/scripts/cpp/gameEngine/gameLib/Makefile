# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=release
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release)
  gameEngineLib_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := gameEngineLib

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

gameEngineLib:
ifneq (,$(gameEngineLib_config))
	@echo "==== Building gameEngineLib ($(gameEngineLib_config)) ===="
	@${MAKE} --no-print-directory -C . -f gameEngineLib.make config=$(gameEngineLib_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f gameEngineLib.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   gameEngineLib"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"