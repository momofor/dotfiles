{
	"coc.preferences.formatOnSaveFiletypes": [
		"javascript",
		"typescript",
		"typescriptreact",
		"json",
		"javascriptreact",
		"typescript.tsx",
		"html",
		"css",
		"python"
	],
	"eslint.filetypes": [
		"javascript",
		"typescript",
		"typescriptreact",
		"javascriptreact",
		"typescript.tsx"
	],
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"prettier.printWidth": 80,

	"python.formatting.provider": "black"
}
